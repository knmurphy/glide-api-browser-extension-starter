#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/knmurphy/Documents/PROJECTS/glide-api-browser-extension-starter/node_modules/.pnpm/@glideapps+tables@1.0.21/node_modules/@glideapps/tables/dist/node_modules:/Users/knmurphy/Documents/PROJECTS/glide-api-browser-extension-starter/node_modules/.pnpm/@glideapps+tables@1.0.21/node_modules/@glideapps/tables/node_modules:/Users/knmurphy/Documents/PROJECTS/glide-api-browser-extension-starter/node_modules/.pnpm/@glideapps+tables@1.0.21/node_modules/@glideapps/node_modules:/Users/knmurphy/Documents/PROJECTS/glide-api-browser-extension-starter/node_modules/.pnpm/@glideapps+tables@1.0.21/node_modules:/Users/knmurphy/Documents/PROJECTS/glide-api-browser-extension-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/knmurphy/Documents/PROJECTS/glide-api-browser-extension-starter/node_modules/.pnpm/@glideapps+tables@1.0.21/node_modules/@glideapps/tables/dist/node_modules:/Users/knmurphy/Documents/PROJECTS/glide-api-browser-extension-starter/node_modules/.pnpm/@glideapps+tables@1.0.21/node_modules/@glideapps/tables/node_modules:/Users/knmurphy/Documents/PROJECTS/glide-api-browser-extension-starter/node_modules/.pnpm/@glideapps+tables@1.0.21/node_modules/@glideapps/node_modules:/Users/knmurphy/Documents/PROJECTS/glide-api-browser-extension-starter/node_modules/.pnpm/@glideapps+tables@1.0.21/node_modules:/Users/knmurphy/Documents/PROJECTS/glide-api-browser-extension-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/client-generator.js" "$@"
else
  exec node  "$basedir/../../dist/client-generator.js" "$@"
fi
